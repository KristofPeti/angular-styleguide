{
  "name": "angular-redux-shop",
  "version": "1.0.0",
  "license": "MIT",
  "private": true,
  "description": "Sample project for ZDI Angular Styleguide",
  "keywords": [
    "angular",
    "styleguide"
  ],
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "build:prod": "ng build --configuration=production --base-href=/angular-styleguide/",
    "test": "ng test",
    "test:ci": "ng test --watch=false --browsers=ChromeHeadless",
    "lint": "ng lint",
    "stylelint": "stylelint src/**/{*.scss,*.css}",
    "e2e:playwright": "(set TS_NODE_PROJECT=./e2e-playwright/tsconfig.json) && playwright test --config=./e2e-playwright/playwright.config.ts",
    "e2e:playwright:ci": "(set TS_NODE_PROJECT=./e2e-playwright/tsconfig.json) && playwright test --config=./e2e-playwright/playwright.config.ci.ts",
    "e2e:playwright:ui": "(set TS_NODE_PROJECT=./e2e-playwright/tsconfig.json) && playwright test --ui --config=./e2e-playwright/playwright.config.ts",
    "e2e:wdio": "(set TS_NODE_PROJECT=./e2e-wdio/tsconfig.json) && wdio run ./e2e-wdio/wdio.conf.ts",
    "e2e:wdio:ci": "start-server-and-test start http://localhost:4200 e2e:wdio",
    "galen": "galen",
    "layout-tests:browserstack": "bash ./layout-tests/run-tests-on-browserstack.sh",
    "prepare": "husky install"
  },
  "dependencies": {
    "@angular/animations": "16.2.9",
    "@angular/common": "16.2.9",
    "@angular/compiler": "16.2.9",
    "@angular/core": "16.2.9",
    "@angular/forms": "16.2.9",
    "@angular/platform-browser": "16.2.9",
    "@angular/platform-browser-dynamic": "16.2.9",
    "@angular/router": "16.2.9",
    "@fortawesome/angular-fontawesome": "0.13.0",
    "@fortawesome/fontawesome-svg-core": "6.4.2",
    "@fortawesome/free-solid-svg-icons": "6.4.2",
    "@ngrx/effects": "16.3.0",
    "@ngrx/store": "16.3.0",
    "@ngrx/store-devtools": "16.3.0",
    "bootstrap": "5.3.2",
    "rxjs": "7.8.1",
    "tslib": "2.6.2",
    "zone.js": "0.13.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "16.2.6",
    "@angular-eslint/builder": "16.2.0",
    "@angular-eslint/eslint-plugin": "16.2.0",
    "@angular-eslint/eslint-plugin-template": "16.2.0",
    "@angular-eslint/schematics": "16.2.0",
    "@angular-eslint/template-parser": "16.2.0",
    "@angular/cli": "16.2.6",
    "@angular/compiler-cli": "16.2.9",
    "@angular/language-service": "16.2.9",
    "@ngrx/schematics": "16.3.0",
    "@playwright/test": "1.38.1",
    "@types/jasmine": "5.1.1",
    "@types/node": "20.8.7",
    "@typescript-eslint/eslint-plugin": "6.8.0",
    "@typescript-eslint/parser": "6.8.0",
    "@wdio/cli": "8.19.0",
    "@wdio/jasmine-framework": "8.19.0",
    "@wdio/local-runner": "8.19.0",
    "@wdio/spec-reporter": "8.19.0",
    "chromedriver": "119.0.1",
    "eslint": "8.51.0",
    "eslint-config-prettier": "9.0.0",
    "eslint-plugin-jasmine": "4.1.3",
    "galenframework-cli": "2.4.4-build43",
    "husky": "8.0.3",
    "jasmine-core": "5.1.1",
    "karma": "6.4.2",
    "karma-chrome-launcher": "3.2.0",
    "karma-coverage": "2.2.1",
    "karma-jasmine": "5.1.0",
    "karma-jasmine-html-reporter": "2.1.0",
    "playwright": "1.38.1",
    "postcss": "8.4.31",
    "prettier": "2.8.8",
    "pretty-quick": "3.1.3",
    "start-server-and-test": "2.0.3",
    "stylelint": "15.11.0",
    "stylelint-config-sass-guidelines": "10.0.0",
    "stylelint-config-standard-scss": "11.0.0",
    "stylelint-no-unsupported-browser-features": "7.0.0",
    "ts-node": "10.9.1",
    "typescript": "5.1.6",
    "wdio-chromedriver-service": "8.1.1"
  }
}
